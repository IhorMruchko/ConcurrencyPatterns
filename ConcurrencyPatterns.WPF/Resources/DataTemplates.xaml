<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:ConcurrencyPatterns.WPF.Views"
                    xmlns:viewModels="clr-namespace:ConcurrencyPatterns.WPF.ViewModels">
    <DataTemplate DataType="{x:Type viewModels:ActiveObjectThreadExampleViewModel}">
        <views:ActiveObjectView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:BalkingObjectThreadExampleViewModel}">
        <views:BalkingObjectView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:SingletonObjectThreadExampleViewModel}">
        <views:SingletonObjectView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:GuardedSuspensionThreadExampleViewModel}">
        <views:GuardedSuspensionView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:BarrierObjectThreadExampleViewModel}">
        <views:BarrierObjectView/>
    </DataTemplate>
</ResourceDictionary>